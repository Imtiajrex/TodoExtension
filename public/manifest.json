{
	"manifest_version": 2,
	"name": "StrictDo",
	"description": "A to do extension that reminds you to do things",
	"version": "1.0",
	"icons": [
		{
			"src": "favicon.ico"
		},
		{
			"src": "logo192.png"
		},
		{
			"src": "logo512.png"
		}
	],
	"start_url": ".",
	"display": "standalone",
	"theme_color": "#000000",
	"background_color": "#ffffff",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["notifications", "alarms"]
}
